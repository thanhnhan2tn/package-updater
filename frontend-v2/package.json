{
  "name": "dependency-checker-frontend",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-tooltip": "1.1.6",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "react": "^19",
    "react-dom": "^19",
    "react-scripts": "5.0.1",
    "tailwind-merge": "^2.5.5",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "typescript": "^5.8.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ],
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx",
            ".json"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
